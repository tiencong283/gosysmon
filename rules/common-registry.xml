<Sysmon schemaversion="4.30">
    <EventFiltering>
        <RuleGroup name="" groupRelation="or">
            <RegistryEvent onmatch="exclude">
                <!-- low information noise -->
                <EventType condition="is">CreateKey</EventType>
                <EventType condition="is">DeleteValue</EventType>
            </RegistryEvent>
        </RuleGroup>
    </EventFiltering>
</Sysmon>