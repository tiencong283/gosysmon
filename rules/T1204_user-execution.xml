<Sysmon schemaversion="4.30">
    <EventFiltering>
        <RuleGroup groupRelation="or">
			<ProcessCreate onmatch="include">
				<Rule name="technique_id=T1204" groupRelation="or">
					<OriginalFileName condition="is">Exell.exe</OriginalFileName>
					<OriginalFileName condition="is">WinWord.exe</OriginalFileName>
					<OriginalFileName condition="is">OUTLOOK.EXE</OriginalFileName>
					<OriginalFileName condition="is">POWERPNT.EXE</OriginalFileName>
					<OriginalFileName condition="is">MSACCESS.EXE</OriginalFileName>
					<OriginalFileName condition="is">MSPUB.EXE</OriginalFileName>
					<OriginalFileName condition="is">INFOPATH.EXE</OriginalFileName>
					<OriginalFileName condition="is">VISIO.EXE</OriginalFileName>
					<OriginalFileName condition="is">VPREVIEW.EXE</OriginalFileName>
					<OriginalFileName condition="is">PPTVIEW.EXE</OriginalFileName>
					<OriginalFileName condition="is">OIS.EXE</OriginalFileName>
					<OriginalFileName condition="is">FoxitReader.exe</OriginalFileName>
					<OriginalFileName condition="is">bash.exe</OriginalFileName>
					<OriginalFileName condition="is">cscript.exe</OriginalFileName>
					<OriginalFileName condition="is">wscript.exe</OriginalFileName>
					<OriginalFileName condition="is">CertUtil.exe</OriginalFileName>
					<OriginalFileName condition="is">desktopimgdownldr.exe</OriginalFileName>
					<OriginalFileName condition="is">winrs.exe</OriginalFileName>
				</Rule>
				<Rule name="technique_id=T1204" groupRelation="or">
					<CommandLine condition="contains">.doc</CommandLine>
					<CommandLine condition="contains">.pdf</CommandLine>
					<CommandLine condition="contains">.xls</CommandLine>
					<CommandLine condition="contains">.rtf</CommandLine>
					<CommandLine condition="contains">.scr</CommandLine>
					<CommandLine condition="contains">.lnk</CommandLine>
					<CommandLine condition="contains">.pif</CommandLine>
					<CommandLine condition="contains">.cpl</CommandLine>
					<CommandLine condition="contains">.exe</CommandLine>
					<CommandLine condition="contains">.vsdx</CommandLine>
				</Rule>
			</ProcessCreate>
        </RuleGroup>
    </EventFiltering>
</Sysmon>
